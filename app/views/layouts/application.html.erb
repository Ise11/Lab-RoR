<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div>
    <% if current_user %>
      Zalogowany jako <%= current_user.email %>
      <%= link_to "Wyloguj", session_path("current"), method: "delete" %> |
    <% else %>
      <%= link_to "Rejestracja", new_user_path %> |
      <%= link_to "Logowanie", new_session_path %>
    <% end %>
  </div>


<%= yield %>

</body>
</html>
