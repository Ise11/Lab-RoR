<h1>Log in</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag sessions_path do %>
    <div class="field">
        <%= label_tag :email %>
        <%= text_field_tag :email, params[:email] %>
    </div>
       <div class="field">
        <%= label_tag :password %>
        <%= text_field_tag :password, params[:email] %>
    </div>
    <div class="actions"><%= submit_tag "Log_in" %></div>
    <% end %>
    
